      <Button fx:id="btnAdd" disable="true" layoutX="184.0" layoutY="109.0" mnemonicParsing="false" onAction="#addOrder" prefHeight="36.0" prefWidth="99.0" style="-fx-background-color: #36384f; -fx-border-color: #fc9d4d; -fx-border-radius: 5; -fx-border-width: 3;" text="Добавить" textFill="#fffde0">
