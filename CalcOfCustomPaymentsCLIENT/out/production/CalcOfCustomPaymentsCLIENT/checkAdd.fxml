<AnchorPane prefHeight="160.0" prefWidth="300.0" style="-fx-background-color: #36384f;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.CheckAddController">
    <children>
        <Button layoutX="185.0" layoutY="15.0" mnemonicParsing="false" onAction="#back" prefHeight="36.0" prefWidth="99.0" style="-fx-background-color: #36384f; -fx-border-color: #fc9d4d; -fx-border-radius: 5; -fx-border-width: 3;" text="Назад" textFill="#fffde0">
         <font>
            <Font name="Arial Black" size="13.0" />
         </font></Button>
        <Button fx:id="btnAccept" layoutX="185.0" layoutY="62.0" mnemonicParsing="false" onAction="#accept" prefHeight="36.0" prefWidth="99.0" style="-fx-background-color: #36384f; -fx-border-color: #fc9d4d; -fx-border-radius: 5; -fx-border-width: 3;" text="Принять" textFill="#fffde0">
         <font>
            <Font name="Arial Black" size="13.0" />
         </font></Button>
      <TextField fx:id="tfPassport" layoutX="24.0" layoutY="54.0" prefHeight="26.0" prefWidth="138.0" promptText="№ паспорта" style="-fx-background-color: #fffde0; -fx-border-radius: 2; -fx-border-width: 3; -fx-border-color: #fc9d4d;">
         <font>
            <Font name="Arial Black" size="10.0" />
