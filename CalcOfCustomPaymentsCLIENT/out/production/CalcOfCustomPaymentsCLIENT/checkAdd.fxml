      <TextField fx:id="tfCount" layoutX="99.0" layoutY="111.0" prefHeight="26.0" prefWidth="62.0" promptText="Кол-во" style="-fx-background-color: #fffde0; -fx-border-radius: 2; -fx-border-width: 3; -fx-border-color: #fc9d4d;">
