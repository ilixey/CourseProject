<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="110.0" prefWidth="285.0" style="-fx-background-color: #36384f;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.CheckClientPassportController">
   <children>
      <Label layoutX="16.0" layoutY="2.0" prefHeight="26.0" prefWidth="152.0" text="Проверка клиента" textFill="#fffde0">
         <font>
            <Font name="Arial Black" size="14.0" />
         </font></Label>
      <TextField fx:id="tfNumPassport" layoutX="15.0" layoutY="62.0" prefHeight="26.0" prefWidth="135.0" promptText="№ паспорта" style="-fx-background-color: #fffde0; -fx-border-radius: 2; -fx-border-width: 3; -fx-border-color: #fc9d4d;">
         <font>
            <Font name="Arial Black" size="12.0" />
         </font></TextField>
      <Button fx:id="btnBack" layoutX="196.0" layoutY="13.0" mnemonicParsing="false" onAction="#back" prefHeight="26.0" prefWidth="66.0" style="-fx-background-color: #36384f; -fx-border-radius: 5; -fx-border-width: 3; -fx-border-color: #fc9d4d;" text="Назад" textFill="#fffde0">
         <font>
            <Font name="Arial Black" size="12.0" />
         </font></Button>
      <Button fx:id="btnCheckClient" layoutX="161.0" layoutY="62.0" mnemonicParsing="false" onAction="#check" prefHeight="26.0" prefWidth="102.0" style="-fx-background-color: #36384f; -fx-border-radius: 5; -fx-border-width: 3; -fx-border-color: #fc9d4d;" text="Проверить" textFill="#fffde0">
         <font>
            <Font name="Arial Black" size="12.0" />
         </font></Button>
      <Label layoutX="15.0" layoutY="28.0" text="по № паспорта" textFill="#fffde0">
         <font>
            <Font name="Arial Black" size="14.0" />
         </font></Label>
   </children>
</AnchorPane>
